{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": "airbnb-base",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-console": "off",
		"linebreak-style": ["error", "windows"],
		"import/no-extraneous-dependencies": 0,
		"no-underscore-dangle": 0,
		"import/no-unresolved": 0,
		"import/extensions": 0
	}
}
